    "start": "set NODE_ENV=production&& node src/server/server",
    "dev": "set NODE_ENV=dev&&node src/server/server",
    "prodNew": "NODE_ENV=production node src/server/main.js",
    "start:server": "ts-node src/server/index.ts",
    "start:server:watch": "nodemon --exec yarn start:server -e ts",
    "build": "set NODE_ENV=production&& webpack --config ./webpack.config.prod.js",
    "buildDevSer": "set NODE_ENV=development&& webpack --config ./config/webpack.dev-server.js",
    "buildDev": "set NODE_ENV=development&& webpack --config ./config/webpack.dev-client.js"
